<script type='text/javascript' src='http://www.google.com/jsapi<%= @api_key %>'></script>
<script type='text/javascript'>
  google.load('visualization', '1', {'packages':[<%= @viz_package_names.map{|p|"'" + p.downcase + "'"}.join(',') -%>]});
  google.setOnLoadCallback(drawChart);
  
  
  function createDataTable(data_pkg, columns){
    var dataTable = new google.visualization.DataTable();
    for(column in columns){
      if(data_pkg['columns'][columns[column]] != undefined){
        dataTable.addColumn(data_pkg['columns'][columns[column]], columns[column]);
      } 
    }
    
    dataTable.addRows(data_pkg['data'].length);
    for(var i = 0; i < data_pkg['data'].length; ++i){
      for(var j = 0; j < columns.length; ++j){
        dataTable.setValue(i, j, data_pkg['data'][i][columns[j]]);
      }
    }
    return dataTable;
  }
  
  <%- @datas.each do |k, data| -%>
    var <%= data.data_name %> = <%= data.to_json %>;
  <%- end -%>
  
  function drawChart() {    
    <%= @visualizations %>
  }
</script>